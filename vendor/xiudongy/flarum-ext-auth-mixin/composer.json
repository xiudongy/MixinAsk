{
    "name": "xiudongy/flarum-ext-auth-mixin",
    "description": "Allow users to log in with Mixin",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "Xiudongy",
            "email": "xiudongy@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/flarum/core/issues",
        "source": "https://github.com/flarum/flarum-ext-auth-xiudongy"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6",
        "league/oauth1-client": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Flarum\\Auth\\Mixin\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Mixin Login",
            "icon": {
                "name": "Mixin",
                "backgroundColor": "#55ADEE",
                "color": "#fff"
            }
        }
    }
}
